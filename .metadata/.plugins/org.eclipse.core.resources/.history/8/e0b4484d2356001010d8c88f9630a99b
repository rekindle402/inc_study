<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>BoardApp</display-name>
<!-- web.xml은 톰켓과 같은 웹 컨테이너가 가동될떄 읽혀진다. 따라서 가동 이후에 XML이 변경되면 재가동해야 한다 -->  

  <servlet>
  	<servlet-name>registServlet</servlet-name>
  	<servlet-class>package com.sinse.boardapp.controller.RegistServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>registServlet</servlet-name>
  	<url-pattern>/notice/regist</url-pattern>
  </servlet-mapping>
</web-app>